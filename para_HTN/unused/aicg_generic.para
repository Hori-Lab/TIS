* Parameters for AICG2 simulation
<<<< aicg2_para
* local parameters
cbd_aicg2 = 110.40

* gaussian width
wid_aicg13 = 0.15
wid_aicg14 = 0.15

* for L_AICG2_PLUS
wid_dih = 0.15

** iflag_scale_aicg2=0:  !recomended
** average contact energies are scaled to "ave_(n)loc"; default
**
** iflag_scale_aicg2=1:
** all-atom contact energiesare are scaled by "gen_(n)loc"
** gen_(n)loc has the unit of 1/(kcal/mol)
iflag_scale_aicg2 = 1

** used when iflag_scale_aicg2 = 0
ave_caicg2_loc = 1.46
ave_caicg2_nloc = 0.60

* for L_AICG2_PLUS
ave_caicg2plus_13 = 1.72
ave_caicg2plus_14 = 1.23
ave_caicg2plus_nloc = 0.55

** used when iflag_scale_aicg2 = 1
gen_caicg2_loc = 0.99
gen_caicg2_nloc = 0.37

* for L_AICG2_PLUS
gen_caicg2plus_13 = 1.11
gen_caicg2plus_14 = 0.87
